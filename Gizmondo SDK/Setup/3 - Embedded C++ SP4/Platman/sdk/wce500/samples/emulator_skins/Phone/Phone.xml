<?xml version="1.0" encoding="UTF-8" ?>
<skin>
	<view
		titleBar ="Windows CE.NET Cell Phone"
		
		displayPosX="36"
		displayPosY="67"
		
		displayWidth="232"
		displayHeight="279"

		displayDepth="16"

		mappingImage="Phone_map.bmp"
		normalImage="Phone_up.bmp"
		downImage= "Phone_down.bmp"

	>


		<button
				
			toolTip="Pad Up"
			onClick="Key_Up"
			mappingColor="0xFFB6C1"
		/>	

